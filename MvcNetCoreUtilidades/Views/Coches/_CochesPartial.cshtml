@model IEnumerable<MvcNetCoreUtilidades.Models.Coche>

@{
    ViewData["Title"] = "_CochesPartial";
}

<h1>_Coches con AJAX</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>IDCOCHE</th>
            <th>MARCA</th>
            <th>MODELO</th>
            <th>IMAGEN</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdCoche)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modelo)
            </td>
            <td>
                <img src="@item.Imagen" style="width: 150px; height: 150px;"/>
            </td>
        </tr>
}
    </tbody>
</table>
